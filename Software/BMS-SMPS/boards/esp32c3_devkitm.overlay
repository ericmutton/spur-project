&i2c0 {
    charger: bq24190@6b {
        compatible = "ti,bq24190";
        status = "okay";
        reg = <0x6B>; // I2C address of the BQ24190
        constant-charge-current-max-microamp = <2000000>; // Set to the maximum charge current in microamperes
        constant-charge-voltage-max-microvolt = <4200000>; // Set to the maximum charge voltage in microvolts (e.g., 4.2V)
    };
};